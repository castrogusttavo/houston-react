{
  "name": "@houstonicons/react",
  "version": "1.0.5",
  "description": "Houston Icons React Component Library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "lint": "eslint src/**/*.ts* --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/castrogusttavo/houston-react.git"
  },
  "bugs": {
    "url": "https://github.com/castrogusttavo/houston-react.git/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "icons",
    "react-icons",
    "ui-components",
    "design-system",
    "vector-icons",
    "react-component-library",
    "web-icons",
    "scalable-icons",
    "customizable-icons",
    "icon-library"
  ],
  "author": "castrogusttavo",
  "dependencies": {
    "fast-glob": "^3.3.2",
    "glob": "^11.0.0",
    "jsonfile": "^6.1.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@hello.softis/eslint-config": "^1.0.1",
    "@types/react": "^18.3.3",
    "eslint": "^8.57.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4",
    "vite": "^5.3.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "npm@20.0.0",
  "license": "CC0-1.0"
}
